													required >
													<option value="">
														Selecione
													</option>
													<option value="1">
														Integrado em Administração
													</option>
													<option value="2">
														Integrado em Informática para Internet
													</option>
													<option value="3">
														Integrado em Manutenção e Suporte em Informática
													</option>
													<option value="4">
														Integrado em Informática (generalista)
													</option>
													<option value="5">
														Superior - TADS
													</option>
													<option value="6">
														Superior - REDES
													</option>
													<option value="7">
														Superior - GESTÃO PÚBLICA
													</option>
													<option value="8">
														Superior - COMEX
													</option>
												</select>
							                </div>
							            </div>
						            </div>
