													required >
													<option value="" >
														Selecione
													</option>
													<option value="1">
														1º Período/Ano
													</option>
													<option value="2">
														2º Período/Ano
													</option>
													<option value="3">
														3º Período/Ano
													</option>
													<option value="4">
														4º Período/Ano
													</option>
													<option value="5">
														5º Período/Ano
													</option>
													<option value="6">
